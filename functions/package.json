{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "lint": "eslint .",
    "serve": "firebase emulators:start --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test:all": "mocha modules/*.test.js",
    "test:ci": "npm install && npm run test:offline",
    "test:offline": "mocha modules/**/offline/**/*.test.js",
    "test:offline:http": "mocha modules/**/offline/http/*.test.js",
    "test:offline:unit": "mocha modules/**/offline/unit/*.test.js",
    "test:online": "mocha modules/**/offline/**/*.test.js",
    "test:online:http": "mocha modules/**/offline/http/*.test.js",
    "test:online:unit": "mocha modules/**/offline/unit/*.test.js"
  },
  "engines": {
    "node": "10"
  },
  "main": "index.js",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "firebase-admin": "^8.10.0",
    "firebase-functions": "^3.6.1",
    "lodash": "^4.17.20",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "7.2.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "4.0.0",
    "firebase-functions-test": "^0.2.2",
    "mocha": "^8.1.3",
    "sinon": "^9.2.0"
  },
  "private": true
}
